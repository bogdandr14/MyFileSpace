{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\v11.0;Database=dsa;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "UseConnection": "DefaultConnection",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": ".\\Logs\\log.txt",
          "rollingInterval": "Day"
        }
      }
      //Uncomment this section if you'd like to push your logs to Azure Application Insights
      //Full list of Serilog Sinks can be found here: https://github.com/serilog/serilog/wiki/Provided-Sinks
      //{
      //  "Name": "ApplicationInsights",
      //  "Args": {
      //    "instrumentationKey": "", //Fill in with your ApplicationInsights InstrumentationKey
      //    "telemetryConverter": "Serilog.Sinks.ApplicationInsights.Sinks.ApplicationInsights.TelemetryConverters.TraceTelemetryConverter, Serilog.Sinks.ApplicationInsights"
      //  }
      //}
    ]
  },
  "FileStorage": {
    "DirectoryPath": "",
    "EncryptionKey": "",
    "AzureBlobStorageName": "myfilespacestore",
    "AzureBlobStorageAccessKey": ""
  },
  "TokenGenerator": {
    "Issuer": "MyFileSpace",
    "Audience": "MyFileSpace",
    "Passphrase": ""
  },
  "DistributedCache": {
    "BypassCache": null,
    "CacheLifeSpan": "0.00:30:00",
    "MemorySizeLimit": "400000"
  },
  "AzureKeyVault": {
    "TenantId": "",
    "ClientId": "",
    "ClientSecretId": "",
    "Uri": "https://myfilespace-key-vault.vault.azure.net/"
  },
  "AllowedHosts": "*"
}
